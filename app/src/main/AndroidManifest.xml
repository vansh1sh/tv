<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.com.example.vansh.onlinefreetv">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FootballClass" android:label="cricket class"></activity>
        <activity android:name=".CricketClass" android:label="cricket class"></activity>
        <activity android:name=".WebviewCricket" android:label="webview cricket" ></activity>
        <activity android:name=".WebviewFootball" android:label="webview cricket" ></activity>
    </application>

</manifest>
